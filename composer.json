{
    "name": "imranismail/rapido",
    "type": "wordpress-theme",
    "description": "Rapido Wordpress Theme Framework.",
    "keywords": ["wordpress", "theme", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Imran Ismail",
            "email": "imran.codely@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "installer-paths": {
            "../../plugins/{$name}/": ["wpackagist-plugin/*", "type:wordpress-plugin"]
        }
    },
    "require": {
        "php": ">=5.4.0",
        "wpackagist-plugin/timber-library": "0.21.*",
        "wpackagist-plugin/polylang": "1.7.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ]
}
